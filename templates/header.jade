nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
	.container
		.navbar-header
			button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
				span.sr-only Toggle navigation
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href='index.html')= name
		div#bs-example-navbar-collapse-1.collapse.navbar-collapse
			ul.nav.navbar-nav
				li
					a(href='api.html') API
				li.dropdown
					a.dropdown-toggle(data-toggle='dropdown', href='#', role='button') Documents
						span.caret
					ul.dropdown-menu(role='menu')
						each _, doc in docs
							li
								a(href=doc+'.html')= doc
.jumbotron
	.container
		h1!= name + '<small> v' + version + '</small>'
		p= description